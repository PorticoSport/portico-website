{
  "redirects": [
    {
      "source": "/(.*)/",
      "destination": "/$1",
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "destination": "/$1",
      "has": [{ "type": "header", "key": "x-lowercase", "value": "true" }],
      "statusCode": 301
    },
    {
      "source": "/panoramic-force-80",
      "destination": "/tournament-plus",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1",
      "has": [{ "type": "header", "key": "x-lowercase", "value": "true" }]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "x-lowercase", "value": "true" }]
    }
  ]
}
