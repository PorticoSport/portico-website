---
import Section from '@/shared/components/Section.astro';

const { countries, title, subtitle } = Astro.props
const projects = countries.filter(({ projects }) => projects).map(({ projects }) => projects).flat()
---

<Section heading={title} subheading={subtitle}>
  {projects.map(({projectLogo, projectName})=> <img src={projectLogo} alt={projectName} />)}
</Section>