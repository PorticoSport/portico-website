---
import PostPage from "@/shared/components/pages/PostPage.astro"
import { LANGUAGES } from "@/shared/i18n/constants"
import { getI18N } from "@/shared/i18n";

const { slug, language } = Astro.params;
const i18n = getI18N({ language })

interface Props {
  language: string;
}

export const prerender = false

---

<PostPage slug={slug || ''} language={LANGUAGES.EN} 
  BreadcrumLabel= {i18n.PAGES.PADEL_NEWS.ROUTE.LABEL}
  BreadcrumbUrl= {i18n.PAGES.PADEL_NEWS.ROUTE.URL}
/>
