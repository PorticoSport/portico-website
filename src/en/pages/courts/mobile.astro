---
import CourtDetailPage from "@/shared/components/pages/CourtDetailPage.astro";
import { LANGUAGES } from "@/shared/i18n/constants";
import { getI18N } from "@/shared/i18n";
import canopiesGallery from "@/en/data/canopies-gallery.json"


const language = LANGUAGES.EN;
const i18n = getI18N({ language });

const court = {
  ...i18n.COURTS.COURT1,
};
---

<CourtDetailPage
  language={language}
  video="/courts-detail/mobile/mobile-court-video.mp4"
  court="COURT1"
  gallery={canopiesGallery}
/>
