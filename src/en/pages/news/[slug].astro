---
import PostPage from "@/shared/components/pages/PostPage.astro"
import { LANGUAGES } from "@/shared/i18n/constants"
const { slug } = Astro.params;

interface Props {
  language: string;
}

export const prerender = false

---

<PostPage slug={slug || ''} language={LANGUAGES.EN} BreadcrumLabel= {i18n.PAGES.NEWS.ROUTE.LABEL}
  BreadcrumbUrl= {i18n.PAGES.NEWS.ROUTE.URL}/>
